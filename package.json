{
  "name": "gatsby-typescript-starter-blog",
  "description": "Gatsby Typescript Starter Blog",
  "version": "1.0.0",
  "author": "Frank Ahn <frankahn9@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier \"{,!(node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
    "format:check": "npm run format -- --check",
    "format:write": "npm run format -- --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "dependencies": {
    "gatsby": "^3.0",
    "gatsby-plugin-catch-links": "^3.0.0",
    "gatsby-plugin-google-fonts-v2": "^0.0.6",
    "gatsby-plugin-image": "^1.0.1",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-postcss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.1",
    "gatsby-plugin-typescript": "^3.0.0",
    "gatsby-remark-autolink-headers": "^3.0.0",
    "gatsby-remark-copy-linked-files": "^3.0.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^4.0.0",
    "gatsby-remark-responsive-iframe": "^3.0.0",
    "gatsby-remark-vscode": "^3.2.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-json": "^3.0.0",
    "gatsby-transformer-remark": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "typescript": "^3.9.7",
    "vscode-themes": "Microsoft/vscode-themes"
  },
  "devDependencies": {
    "@types/node": "^10.17.13",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "babel-preset-gatsby": "^1.0.0",
    "cypress": "^6.5.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^1.7.6",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frnki/gatsby-typescript-starter-blog"
  }
}
